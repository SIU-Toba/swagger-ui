  <ul class='operations' >
    <li class='{{{escape method}}} operation' id='{{{escape parentId}}}_{{{escape nickname}}}'>
      <div class='heading'>
        <h3>
          <span class='http_method'>
          <a href='#!/{{sanitize parentId}}/{{sanitize nickname}}' class="toggleOperation">{{{escape method}}}</a>
          </span>
          <span class='path'>
          <a href='#!/{{sanitize parentId}}/{{sanitize nickname}}' class="toggleOperation {{#if deprecated}}deprecated{{/if}}">{{{escape path}}}</a>
          </span>
        </h3>
        <ul class='options'>
          {{#if showOperationIds}}
          <li>
          <a href='#!/{{sanitize parentId}}/{{sanitize nickname}}' class="toggleOperation"><span class="nickname">{{{escape nickname}}}()</span></a>
          </li>
          {{/if}}
          <li>
          <a href='#!/{{sanitize parentId}}/{{sanitize nickname}}' class="toggleOperation"><span class="markdown">{{{escape summary}}}</span></a>
          </li>
        </ul>
      </div>
      <div class='content' id='{{sanitize parentId}}_{{sanitize nickname}}_content' style='display:none'>
        {{#if deprecated}}
            <h4><span data-sw-translate>Cuidado: Deprecado</span></h4>
        {{/if}}
        {{#if description}}
        <h4><span data-sw-translate>Notas de Implementación</span></h4>
        <div class="markdown">{{{sanitize description}}}</div>
        {{/if}}
        {{#if security}}
            <div class='authorize-wrapper authorize-wrapper_operation'></div>
        {{/if}}
        {{#if type}}
          <div class="response-class">
            <h4><span data-sw-translate>Respuesta éxitosa</span> (<span data-sw-translate>Status</span> {{{escape successCode}}})</h4>
              {{#if successDescription}}<div class="markdown">{{{sanitize successDescription}}}</div>{{/if}}
            <p><span class="model-signature" /></p>
            <br/>
            <div class="response-content-type" />
            </div>
        {{/if}}

        {{#if headers}}
          <h4 data-sw-translate>Encabezados</h4>
          <table class="headers">
            <thead>
              <tr>
                <th style="width: 100px; max-width: 100px" data-sw-translate>Encabezado</th>
                <th style="width: 310px; max-width: 310px" data-sw-translate>Descripción</th>
                <th style="width: 200px; max-width: 200px" data-sw-translate>Tipo</th>
                <th style="width: 320px; max-width: 320px" data-sw-translate>Otros</th>
              </tr>
            </thead>
            <tbody>
              {{#each headers}}
              <tr>
                <td>{{@key}}</td>
                <td>{{{sanitize this.description}}}</td>
                <td>{{{escape this.type}}}</td>
                <td>{{{escape this.other}}}</td>
              </tr>
              {{/each}}
            </tbody>
          </table>
        {{/if}}

        <form accept-charset='UTF-8' class='sandbox'>
          <div style='margin:0;padding:0;display:inline'></div>
          {{#if parameters}}
          <h4 data-sw-translate>Parámetros</h4>
          <table class='fullwidth parameters'>
          <thead>
            <tr>
            <th style="width: 100px; max-width: 100px" data-sw-translate>Parámetro</th>
            <th style="width: 310px; max-width: 310px" data-sw-translate>Valor</th>
            <th style="width: 200px; max-width: 200px" data-sw-translate>Descripción</th>
            <th style="width: 100px; max-width: 100px" data-sw-translate>Tipo Parám.</th>
            <th style="width: 220px; max-width: 230px" data-sw-translate>Tipo Dato</th>
            </tr>
          </thead>
          <tbody class="operation-params">

          </tbody>
          </table>
          {{/if}}
          {{#if responseMessages}}
          <div style='margin:0;padding:0;display:inline'></div>
          <h4 data-sw-translate>Respuestas</h4>
          <table class='fullwidth response-messages'>
            <thead>
            <tr>
              <th data-sw-translate>Estado HTTP</th>
              <th data-sw-translate>Motivo</th>
              <th data-sw-translate>Modelo de Respuesta</th>
              <th data-sw-translate>Encabezados</th>
            </tr>
            </thead>
            <tbody class="operation-status">
            </tbody>
          </table>
          {{/if}}
          {{#if isReadOnly}}
          {{else}}
          <div class='sandbox_header'>
            <input class='submit' type='submit' value='Test!' data-sw-translate/>
            <a href='#' class='response_hider' style='display:none' data-sw-translate>Ocultar</a>
            <span class='response_throbber' style='display:none'></span>
          </div>
          {{/if}}
        </form>
        <div class='response' style='display:none'>
          <h4 class='curl'>Curl</h4>
          <div class='block curl'></div>
          <h4 data-sw-translate> URL Pedido</h4>
          <div class='block request_url'></div>
          {{#if showRequestHeaders}}
          <h4 data-sw-translate>Encabezados Pedido</h4>
          <div class='block request_headers'></div>
          {{/if}}
          <h4 data-sw-translate>Cuerpo Respuesta</h4>
          <div class='block response_body'></div>
          <h4 data-sw-translate>Codigo Respuesta</h4>
          <div class='block response_code'></div>
          <h4 data-sw-translate>Encabezados Respuesta</h4>
          <div class='block response_headers'></div>
        </div>
      </div>
    </li>
  </ul>
