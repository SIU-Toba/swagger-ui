<div class='info' id='api_info'>
  {{#if info}}
  <div class="info_title">{{info.title}}</div>
  <div class="info_description markdown">{{{info.description}}}</div>
  {{#if info.termsOfServiceUrl}}<div class="info_tos"><a href="{{info.termsOfServiceUrl}}">Terminos de Servicio</a></div>{{/if}}
  {{#if info.contact.name}}<div class='info_name'>Creado por {{info.contact.name}}</div>{{/if}}
  {{#if info.contact.url}}<div class='info_url'>Ver m√°s en <a href="{{info.contact.url}}">{{info.contact.url}}</a></div>{{/if}}
  {{#if info.contact.email}}<div class='info_email'><a href="mailto:{{info.contact.email}}?subject={{info.title}}">Contactar desarrollador</a></div>{{/if}}
  {{#if info.license}}<div class='info_license'><a href='{{info.license.url}}'>{{info.license.name}}</a></div>{{/if}}
  {{/if}}
</div>
<div class='container' id='group_resources_container'>
    <ul id='group_resources'></ul>

  <div class="footer">
    <br>
    <br>
    <h4 style="color: #999">[ <span style="font-variant: small-caps">url base</span>: {{basePath}}
    {{#if info.version}}
    , <span style="font-variant: small-caps">version api</span>: {{info.version}}
    {{/if}}]
    {{#if validatorUrl}}
    <span style="float:right"><a href="{{validatorUrl}}/debug?url={{url}}"><img id="validator" src="{{validatorUrl}}?url={{url}}"></a>
    </span>
    {{/if}}
    </h4>
    </div>
</div>
